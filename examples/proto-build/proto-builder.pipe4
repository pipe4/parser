

unit BuildProtoInGitOnPush ({
    from    net.Url
    to      net.Url
    lang    string
}) {
    protoReg := regexp.RE2(`[^\/\\]+.proto$`)
    subscription, err := git.Subscribe({url: from, match: protoReg})

    notification := subscription.Each()
    BuildProtoGit({from, to, lang})
}

